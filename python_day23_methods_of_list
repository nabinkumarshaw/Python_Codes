l=[23,45,1,6,34]
print(l)

#Append
l.append(100)
print(l)
#sort
l.sort()
print(l)

#remove
l.remove(100)
print(l)

#sort in descending order
l.sort(reverse=True)
print(l)

#index()
find_idx=l.index(34)
print(find_idx)

#count()
cou=l.count(2)
print(cou)


#copy
m=l.copy()
m[0]=1000
print(l)

#insert()
l.insert(1,2000)
print(l)

#extend()
m=[100,200,300]
l.extend(m)
print(l)
