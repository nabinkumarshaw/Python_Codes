#code for factorial
def factorial(n):
    if(n==0 or n==1):
      return n
    else:
      return n*factorial(n-1)

print(factorial(5))


#recursion code for fibonacci 

def fibonacci(n):
  if(n==0):
    return 0
  if(n==1):
    return 1
  else:
    return fibonacci(n-1) + fibonacci(n-2)

for i in range(10):
  print(fibonacci(i)," ")


