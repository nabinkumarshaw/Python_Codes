info = {"name": "nabin", "age": 21, "Id": "#8765"}
print(info)
print(info["name"])
print(info.get("name"))

#Accessing keys
print(info.keys())

#Accessing values
print(info.values())

for i in info.keys():
  print(f"The key is {i} and the value is {info[i]}")

#Accessing key value pair simultanously

print(info.items())

for key, value in info.items():
  print(f"The key is {key} and the value is {value}")
