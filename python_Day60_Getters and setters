class myclass:
  def __init__(self,value):   #constructor
    self.value=value

  
  def show(self):             #Method
    print(f"value is {self.value}")

  @property                 # getter 
  def getten_value(self):
    return 10*self.value

  @getten_value.setter        #setter
  def setten_value(self,new_value):
    self.value=new_value/10
  

obj= myclass(70)
obj.setten_value=90
print(obj.getten_value)
obj.show()



