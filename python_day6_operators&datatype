# variable
a=1
print(a)
b="nabin"
print(b)
c=True
print(c)
d=None
print(d)

#datatype
#list is mutable in nature
list1=[[12,["apple","mango"],[1,34],23,45,[90.89]]]
print(list1)

list1[0]=["green","yellow"]
print(list1)
#tuple is immutable in nature
tuple1=[[12,["apple","mango"],[1,34],23,45,[90.89]]]
print(list1)

#dict
dict1={"name:nabin","canvote:true","age:23"}
print(dict1)
