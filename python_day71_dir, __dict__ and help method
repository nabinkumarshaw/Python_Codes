#The dir() method
x={2,4,1}
print(dir(x))  #it will print all the methods and attributes of an objects

#The __dict__ attribute
class person:
  def __init__(self,name,age):
    self.name=name
    self.age=age

p=person("john",30)
print(p.__dict__)  #It will return in the form of dictionary

#The help() method
print(help(person))
