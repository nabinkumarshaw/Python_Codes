a=input("Enter a number")
try:
  for i in range(1,11):
    print(f"{(int)(a)}*{i}={(int)(a)*i}")
except ValueError:
    print("value error occured")

print("Nabin")


#second Example
try:
    num=input("Enter a number")
    arr=[8,9]
    print(arr[int(num)])
except ValueError:
    print("wrong value passed")
except IndexError:
    print("out of Index")

