class Employee:
  company="Apple"
  def show(self):
    print(f"Employee name is {self.name} and company is {self.company}")

  @classmethod
  def changecompany(cls,newcomp):
    cls.company="Tesla"

e1=Employee()
e1.name="Nabin"
e1.show()
e1.changecompany("Tesla")
e1.show()
print(Employee.company)
