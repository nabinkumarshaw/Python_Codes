import  math
print(math.sqrt(9))


#use of "from" keyword
from math import pi,pow,sqrt,ceil
num=23.890
print(pi)
print(pow(num,2))
print(sqrt(num))
print(ceil(num))

#use of "as" keyword
import math as m
print(m.pi)
print(m.pow(2,3))

